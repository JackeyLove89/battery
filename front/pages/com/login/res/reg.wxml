<van-nav-bar
      title="注册"    
     
      left-text="返回"     
      left-arrow
      bind:click-left="onClickLeft"
    />
<view class="container"> 
 
 <view class="login-icon"> 
 <image class="login-img" src="../../../../images/logo.jpg"></image> 
 </view> 
 <view class="login-from"> 
 
 <!--账号-->
 <view class="inputView"> 
  
  <label class="loginLab">账号</label> 
  <input class="inputText" placeholder="请输入帐号" bindinput="usernameInput" /> 
 </view> 
 <view class="line"></view> 
 
 <!--密码-->
 <view class="inputView"> 

  <label class="loginLab">密码</label> 
  <input class="inputText" password="true" placeholder="请输入密码" bindinput="passwordInput" /> 
 </view> 
 
 <!--按钮-->
 <view class="loginBtnView"> 
  <button class="loginBtn" type="primary" size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login">注册</button> 

 </view> 
 
 </view> 
</view>